{
    "name": "cranux/laravel-dev-template",
    "type": "project",
    "description": "laravel 高效开发模板",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "cranux/laravel-dotenv-editor": "^1.0",
        "fideloper/proxy": "^4.0",
        "fx3costa/laravelchartjs": "^2.6",
        "hhxsv5/laravel-s": "^3.6",
        "jeroennoten/laravel-adminlte": "^3.0",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6.0",
        "league/fractal": "^0.18.0",
        "overtrue/laravel-ueditor": "^1.2",
        "overtrue/laravel-wechat": "^5.0",
        "prettus/l5-repository": "^2.6",
        "spatie/laravel-permission": "^3.4",
        "tymon/jwt-auth": "1.0.0-rc.5",
        "vinkla/hashids": "^7.0",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-buttons": "^4.8",
        "yajra/laravel-datatables-oracle": "^9.8",
        "zedisdog/laravel-schema-extend": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.3",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "hirak/prestissimo": "^0.3.10",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
